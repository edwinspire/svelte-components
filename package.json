{
	"name": "@edwinspire/svelte-components",
	"version": "1.1.0",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edwinspire/svelte-components.git"
	},
	"keywords": [
		"svelte",
		"javascript",
		"table",
		"component"
	],
	"author": "edwinspire",
	"license": "LGPL-3.0",
	"bugs": {
		"url": "https://github.com/edwinspire/svelte-components.git/issues"
	},
	"homepage": "https://github.com/edwinspire/svelte-components.git#readme",
	"description": "Varios componentes para Svelte",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "8.56.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.1.9",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.4.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@codemirror/autocomplete": "^6.18.4",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-javascript": "^6.2.2",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lang-sql": "^6.8.0",
		"@codemirror/lang-xml": "^6.1.0",
		"@codemirror/language": "^6.10.8",
		"@codemirror/state": "^6.5.2",
		"@codemirror/theme-one-dark": "^6.1.2",
		"@codemirror/view": "^6.36.2",
		"@edwinspire/universal-fetch": "https://github.com/edwinspire/universal-fetch.git#v1.0.1",
		"@fortawesome/fontawesome-free": "^6.4.0",
		"@inlang/paraglide-sveltekit": "^0.11.1",
		"bulma": "^1.0.1",
		"codemirror": "^6.0.1",
		"d3": "^7.8.5",
		"events": "^3.3.0",
		"luxon": "^2.0.2",
		"prettier-plugin-sql": "^0.18.1",
		"xlsx": "^0.18.5"
	},
	"importSort": {
		"*.js": {
			"style": "module"
		}
	}
}
