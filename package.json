{
  "name": "@edwinspire/svelte-components",
  "version": "1.1.16",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwinspire/svelte-components.git"
  },
  "keywords": [
    "svelte",
    "javascript",
    "table",
    "component"
  ],
  "author": "edwinspire",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/edwinspire/svelte-components.git/issues"
  },
  "homepage": "https://github.com/edwinspire/svelte-components.git#readme",
  "description": "Varios componentes para Svelte",
  "scripts": {
    "dev": "vite dev",
    "build": "node ./set_version.js && vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.20.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "8.56.0",
    "echarts": "^5.6.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.2.6",
    "publint": "^0.1.9",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.11"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.4",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/language": "^6.10.8",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.36.2",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/code": "^2.9.3",
    "@editorjs/editorjs": "^2.30.8",
    "@editorjs/embed": "^2.7.6",
    "@editorjs/header": "^2.8.8",
    "@editorjs/image": "^2.10.2",
    "@editorjs/inline-code": "^1.5.1",
    "@editorjs/list": "^2.0.6",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/paragraph": "^2.11.7",
    "@editorjs/quote": "^2.7.6",
    "@editorjs/table": "^2.4.4",
    "@edwinspire/universal-fetch": "https://github.com/edwinspire/universal-fetch.git",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "bulma": "^1.0.1",
    "codemirror": "^6.0.1",
    "events": "^3.3.0",
    "luxon": "^2.0.2",
    "marked": "^16.3.0",
    "prettier-plugin-sql": "^0.18.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "importSort": {
    "*.js": {
      "style": "module"
    }
  }
}
