{
	"name": "@edwinspire/svelte-components",
	"version": "1.0.0",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edwinspire/svelte-components.git"
	},
	"keywords": [
		"svelte",
		"javascript",
		"table",
		"component"
	],
	"author": "edwinspire",
	"license": "LGPL-3.0",
	"bugs": {
		"url": "https://github.com/edwinspire/svelte-components.git/issues"
	},
	"homepage": "https://github.com/edwinspire/svelte-components.git#readme",
	"description": "Varios componentes para Svelte",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@codemirror/basic-setup": "^0.20.0",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-javascript": "^6.2.2",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lang-xml": "^6.1.0",
		"@codemirror/state": "^6.4.1",
		"@codemirror/view": "^6.32.0",
		"@edwinspire/universal-fetch": "git+https://github.com/edwinspire/universal-fetch.git",
		"@fortawesome/fontawesome-free": "^6.4.0",
		"bulma": "^1.0.1",
		"codemirror": "^6.0.1",
		"d3": "^7.8.5",
		"events": "^3.3.0",
		"luxon": "^2.0.2",
		"xlsx": "^0.18.5"
	}
}
